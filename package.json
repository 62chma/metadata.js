  "scripts": {
    "build:core": "node ./packages/metadata-core/build.js",
    "build:transition": "node ./packages/metadata-dhtmlx-ui/build.js",
    "build:react-ui": "better-npm-run build:react-ui",
    "build:abstract-ui": "node ./packages/metadata-abstract-ui/build.js",
    "build:adapter-pouchdb": "node ./packages/metadata-pouchdb/build.js",
    "build:redux": "node ./packages/metadata-redux/build.js",
    "build:superlogin": "better-npm-run build:superlogin"
  },
  "betterScripts": {
    "build:react-ui": {
      "command": "node ./packages/metadata-react-ui/src/build.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:superlogin": {
      "command": "babel ./packages/metadata-superlogin/src/superlogin.js --out-file ./packages/metadata-superlogin/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:redux": {
      "command": "babel ./packages/metadata-redux/src --out-file ./packages/metadata-redux/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "bin": {
    "metadata": "./bin/metadata-cli.js"
  },
  "repository": {
    "type": "git",
